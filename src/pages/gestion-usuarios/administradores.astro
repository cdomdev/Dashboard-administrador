---
import LayoutUsers from "../../layouts/LayoutUsers.astro";
import ListAdmin from "../../components/usuarios/ListAdmin.jsx";
const cookies = Astro.cookies;
const authToken = cookies.get("access_token");
const isAuthenticated = authToken !== undefined;

if(!isAuthenticated){
  return Astro.redirect('/auth')
}

---

<LayoutUsers>
   <ListAdmin client:visible/>   
</LayoutUsers>